{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/media/*", "/api/*"]
  },
  "routes": [
    {
      "route": "/media/*",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/api/*",
      "allowedRoles": ["anonymous"],
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"]
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  }
}
